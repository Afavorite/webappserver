<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.demo.service.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.demo.service.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AndroidLogin</servlet-name>
        <servlet-class>com.demo.service.AndroidLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AndroidLogin</servlet-name>
        <url-pattern>/androidlogin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AndroidRegister</servlet-name>
        <servlet-class>com.demo.service.AndroidRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AndroidRegister</servlet-name>
        <url-pattern>/androidregister</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetBoxInfo</servlet-name>
        <servlet-class>com.demo.service.GetBoxInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetBoxInfo</servlet-name>
        <url-pattern>/getboxinfo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>OrderAdd</servlet-name>
        <servlet-class>com.demo.service.OrderAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderAdd</servlet-name>
        <url-pattern>/orderadd</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RPiServlet</servlet-name>
        <servlet-class>com.demo.service.RPiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RPiServlet</servlet-name>
        <url-pattern>/rpiservlet</url-pattern>
    </servlet-mapping>
</web-app>